<?xml version="1.0" encoding="utf-8"?>
<SubmissionSetting xmlns:i="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://schemas.datacontract.org/2004/07/Kooboo.CMS.Sites.Models">
	<Name>Contact</Name>
	<PluginType>Kooboo.CMS.Sites.Extension.ContactUsPlugin, Kooboo.CMS.Sites</PluginType>
	<Settings xmlns:d2p1="http://schemas.microsoft.com/2003/10/Serialization/Arrays">
		<d2p1:KeyValueOfstringstring>
			<d2p1:Key>Body</d2p1:Key>
			<d2p1:Value>&lt;table&gt;
    &lt;tr&gt;
        &lt;td&gt;First Name&lt;/td&gt;
        &lt;td&gt;{FirstName}&lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
        &lt;td&gt;Last Name&lt;/td&gt;
        &lt;td&gt;{FirstName}&lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
        &lt;td&gt;Email&lt;/td&gt;
        &lt;td&gt;{Email}&lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
        &lt;td&gt;Phone Number&lt;/td&gt;
        &lt;td&gt;{phoneNum}&lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
        &lt;td&gt;Job Title&lt;/td&gt;
        &lt;td&gt;{job}&lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
        &lt;td&gt;Country&lt;/td&gt;
        &lt;td&gt;{Country}&lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
        &lt;td&gt;Details&lt;/td&gt;
        &lt;td&gt;{details}&lt;/td&gt;
    &lt;/tr&gt;
&lt;/table&gt;</d2p1:Value>
		</d2p1:KeyValueOfstringstring>
		<d2p1:KeyValueOfstringstring>
			<d2p1:Key>IsBodyHtml</d2p1:Key>
			<d2p1:Value>true</d2p1:Value>
		</d2p1:KeyValueOfstringstring>
		<d2p1:KeyValueOfstringstring>
			<d2p1:Key>Subject</d2p1:Key>
			<d2p1:Value>{subject}</d2p1:Value>
		</d2p1:KeyValueOfstringstring>
		<d2p1:KeyValueOfstringstring>
			<d2p1:Key>From</d2p1:Key>
			<d2p1:Value>{email}</d2p1:Value>
		</d2p1:KeyValueOfstringstring>
	</Settings>
</SubmissionSetting>