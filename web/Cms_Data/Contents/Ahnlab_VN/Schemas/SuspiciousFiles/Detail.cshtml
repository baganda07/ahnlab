@if (Model != null)
{ 
<div>
	<h3 class="title" @ViewHelper.Edit(Model,"Subject")>@Html.Raw(Model.Subject ?? "")</h3>
	<div class="content">
		<div @ViewHelper.Edit(Model,"ProductUUID")>@Html.Raw(Model.ProductUUID ?? "")</div>
		<div @ViewHelper.Edit(Model,"ProductName")>@Html.Raw(Model.ProductName ?? "")</div>
		<div @ViewHelper.Edit(Model,"Name")>@Html.Raw(Model.Name ?? "")</div>
		<div @ViewHelper.Edit(Model,"Email")>@Html.Raw(Model.Email ?? "")</div>
		<div @ViewHelper.Edit(Model,"ContactNumber")>@Html.Raw(Model.ContactNumber ?? "")</div>
		<div @ViewHelper.Edit(Model,"SymptomDetails")>@Html.Raw(Model.SymptomDetails ?? "")</div>
		<div @ViewHelper.Edit(Model,"Attachment")>@Html.Raw(Model.Attachment ?? "")</div>
		<div @ViewHelper.Edit(Model,"Screenshot")>@Html.Raw(Model.Screenshot ?? "")</div>
	</div>
</div>
}